            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": "{\"result\":\"Error\",\"data\":[],\"message\":\"An internal server error occurred while processing the request\"}"
              }
            }
          }
        }
      }
