                    "value": "{\"result\":\"Failure\",\"data\":[{\"result\":\"NotAllItemsFit\",\"bin\":{\"id\":\"custom_small_bin_1\",\"length\":20,\"width\":20,\"height\":15},\"fittedItems\":[{\"id\":\"box_2\",\"length\":12,\"width\":15,\"height\":10}],\"unfittedItems\":[{\"id\":\"box_1\",\"quantity\":1},{\"id\":\"box_2\",\"quantity\":1}],\"fittedBinVolumePercentage\":30.00,\"fittedItemsVolumePercentage\":48.65}]}"
                  },
