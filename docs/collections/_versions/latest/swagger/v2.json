  "components": {
    "schemas": {
      "Bin": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "length": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "width": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "height": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      },
      "BinFitResult": {
        "required": [
          "bin"
        ],
        "type": "object",
        "properties": {
          "result": {
            "$ref": "#/components/schemas/BinFitResultStatus"
          },
          "bin": {
