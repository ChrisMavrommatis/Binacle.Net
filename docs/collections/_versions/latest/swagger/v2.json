    "/api/v2/fit/by-custom": {
      "post": {
        "tags": [
          "Fit"
        ],
        "summary": "Fit by Custom",
        "description": "Perform a bin fitting function using custom bins.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomFitRequest"
              },
              "example": "{\"parameters\":{\"reportFittedItems\":true,\"reportUnfittedItems\":true,\"findSmallestBinOnly\":false},\"bins\":[{\"id\":\"custom_bin_1\",\"length\":10,\"width\":40,\"height\":60},{\"id\":\"custom_bin_2\",\"length\":20,\"width\":40,\"height\":60}],\"items\":[{\"id\":\"box_1\",\"quantity\":2,\"length\":2,\"width\":5,\"height\":10},{\"id\":\"box_2\",\"quantity\":1,\"length\":12,\"width\":15,\"height\":10},{\"id\":\"box_3\",\"quantity\":1,\"length\":12,\"width\":10,\"height\":15}]}"
            }
          },
          "required": true
        },
        "responses": {
          "500": {
            "description": "**Internal Server Error**<br />\n\nIf an unexpected error occurs.\nException details will only be shown when in a development environment.\n\n\n",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "example": "{\"result\":\"Error\",\"data\":[],\"message\":\"An internal server error occurred while processing the request\"}"
              }
            }
          },
          "200": {
            "description": "**OK**<br />\n\nReturns an array of results indicating if a bin can accommodate all the items.\n\n",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FitResponse"
                },
                "examples": {
                  "fullresponse": {
                    "summary": "Full Response",
                    "description": "Response example when you opt in to report all bins and all items.",
                    "value": "{\"result\":\"Success\",\"data\":[{\"result\":\"AllItemsFit\",\"bin\":{\"id\":\"custom_bin_1\",\"length\":10,\"width\":40,\"height\":60},\"fittedItems\":[{\"id\":\"box_1\",\"dimensions\":{\"length\":2,\"width\":5,\"height\":10}},{\"id\":\"box_2\",\"dimensions\":{\"length\":12,\"width\":15,\"height\":10}}],\"unfittedItems\":[],\"fittedItemsVolumePercentage\":100.00,\"fittedBinVolumePercentage\":8.33},{\"result\":\"AllItemsFit\",\"bin\":{\"id\":\"custom_bin_2\",\"length\":20,\"width\":40,\"height\":60},\"fittedItems\":[{\"id\":\"box_1\",\"dimensions\":{\"length\":2,\"width\":5,\"height\":10}},{\"id\":\"box_2\",\"dimensions\":{\"length\":12,\"width\":15,\"height\":10}}],\"unfittedItems\":[],\"fittedItemsVolumePercentage\":100.00,\"fittedBinVolumePercentage\":4.17}]}"
                  },
                  "itemsomittedresponse": {
                    "summary": "Items Omitted Response",
                    "description": "Response example when you omit the items.",
                    "value": "{\"result\":\"Success\",\"data\":[{\"result\":\"AllItemsFit\",\"bin\":{\"id\":\"custom_bin_1\",\"length\":10,\"width\":40,\"height\":60},\"fittedItemsVolumePercentage\":100.00,\"fittedBinVolumePercentage\":8.33},{\"result\":\"AllItemsFit\",\"bin\":{\"id\":\"custom_bin_2\",\"length\":20,\"width\":40,\"height\":60},\"fittedItemsVolumePercentage\":100.00,\"fittedBinVolumePercentage\":4.17}]}"
                  },
                  "smallestbinresponse": {
                    "summary": "Smallest Bin Response",
                    "description": "Response example when you request to report only the smallest bin.",
                    "value": "{\"result\":\"Success\",\"data\":[{\"result\":\"AllItemsFit\",\"bin\":{\"id\":\"custom_bin_1\",\"length\":10,\"width\":40,\"height\":60},\"fittedItemsVolumePercentage\":100.00,\"fittedBinVolumePercentage\":8.33}]}"
                  },
                  "binnotfitresponse": {
                    "summary": "Bin Not Fit Response",
                    "description": "Response example when a bin can't accommodate all the items",
                    "value": "{\"result\":\"Failure\",\"data\":[{\"result\":\"NotAllItemsFit\",\"bin\":{\"id\":\"custom_small_bin_1\",\"length\":20,\"width\":20,\"height\":15},\"fittedItems\":[{\"id\":\"box_2\",\"dimensions\":{\"length\":12,\"width\":15,\"height\":10}}],\"unfittedItems\":[{\"id\":\"box_1\",\"dimensions\":{\"length\":2,\"width\":5,\"height\":10}},{\"id\":\"box_2\",\"dimensions\":{\"length\":12,\"width\":15,\"height\":10}}],\"fittedItemsVolumePercentage\":48.65,\"fittedBinVolumePercentage\":30.00}]}"
                  },
                  "earlyfailresponse": {
                    "summary": "Early fail Response",
                    "description": "Response example when a bin can't accommodate all the items due to an early fail check",
                    "value": "{\"result\":\"Failure\",\"data\":[{\"result\":\"EarlyFail_TotalVolumeExceeded\",\"bin\":{\"id\":\"custom_small_bin_1\",\"length\":10,\"width\":10,\"height\":10},\"fittedItems\":[],\"unfittedItems\":[{\"id\":\"box_2\",\"dimensions\":{\"length\":12,\"width\":15,\"height\":10}},{\"id\":\"box_1\",\"dimensions\":{\"length\":2,\"width\":5,\"height\":10}},{\"id\":\"box_2\",\"dimensions\":{\"length\":12,\"width\":15,\"height\":10}}],\"fittedItemsVolumePercentage\":0.00,\"fittedBinVolumePercentage\":0.00}]}"
                  }
                }
              }
            }
          },
          "400": {
            "description": "**Bad Request**<br />\n\nIf the request is invalid.\n\n",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                },
                "examples": {
                  "validationError": {
                    "summary": "Validation Error",
                    "description": "Example response with validation errors",
                    "value": "{\"result\":\"Error\",\"data\":[{\"$type\":\"FieldValidationError\",\"field\":\"Items[0].Height\",\"error\":\"Must be greater than 0.\"}],\"message\":\"One or more validation errors occurred\"}"
                  },
                  "requestError": {
                    "summary": "Request Error",
                    "description": "Example response when the request is malformed",
                    "value": "{\"result\":\"Error\",\"data\":[{\"$type\":\"ParameterError\",\"parameter\":\"request\",\"message\":\"Malformed request body.\"}],\"message\":\"One or more validation errors occurred\"}"
                  }
                }
              }
            }
          }
        }
      }
    },
